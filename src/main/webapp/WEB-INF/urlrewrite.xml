<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
        <rule>
		<from>/tld/**</from>
		<to>/tld/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
	<rule>
		<from>/output/**</from>
		<to>/output/$1</to>
	</rule>
        <rule>
		<from>/layers/analysis/inter_layer_association.pdf</from>
		<to>/output/inter_layer_association.pdf</to>
	</rule>	
        <rule>
		<from>/jobs.zul</from>
		<to>/jobs.zul</to>
	</rule>	
        <rule>
            <from>/files.zul</from>
            <to>/files.zul</to>
        </rule>	
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/zkau/**</from>
		<to>/zkau/$1</to>
	</rule>
	<rule>
		<from>/zkau</from>
		<to>/zkau</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>
